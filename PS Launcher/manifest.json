{
    "manifest_version": 2,
    "name": "PS Launcher",
    "description": "Social Profiles of Paurav Shah",
    "version": "1.0.0",
    "icons": {
        "16": "icons/favicon-16x16.png",
        "32": "icons/favicon-32x32.png",
        "192": "icons/android-chrome-192x192.png",
        "512": "icons/android-chrome-512x512.png"
    },
    "browser_action": {
        "default_icon": "icons/favicon-16x16.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ]
}